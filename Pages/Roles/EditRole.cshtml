@page
@model Code1stUsersRoles.Pages.Roles.EditRole
@{
    ViewData["Title"] = "EditRole";
}

<h1>@ViewData["Title"]</h1>

<form method="post">
    <label for="roleName">Role Name:</label>
    <input type="text" id="roleName" name="roleName" value="@Model.RoleName" required />
    <br />
    <button type="submit">Update</button>
</form>